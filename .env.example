# Example Environment Variables
# Copy this file to .env.local for local development overrides

# Angular Client Variables (injected into browser bundle)
# Only variables prefixed with NG_APP_ are available in the browser
NG_APP_WEBSOCKET_URL=ws://localhost:8080/ws

# Shared Variables (available to both client and server)
# These are accessible from both Angular client and Node.js server
NG_APP_PACKET_SCRAMBLER_SEED=your-scrambler-seed

# Server Variables (Node.js only)
# These are only available on the server side
BACKEND_PORT=8080